<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.focusgroupheroican"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />
    
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/lollipop_style" >
        
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminActivity"
            android:label="@string/title_activity_admin" >
        </activity>
        <activity
            android:name=".EncuestaActivity"
            android:label="@string/title_activity_encuesta" >
        </activity>
        <activity
            android:name=".AdminEncuestaActivity"
            android:label="@string/title_activity_admin_encuesta" >
        </activity>
        <activity
            android:name=".AdminQuestionActivity"
            android:label="@string/title_activity_admin_question" >
        </activity>
        <activity
            android:name=".RegistroActivity"
            android:label="@string/title_activity_registro" >
        </activity>
        <activity
            android:name=".AdminCommentActivity"
            android:label="@string/title_activity_admin_comment" >
        </activity>
    </application>

</manifest>
